extends layout
block content
    div#content.col-md-9
        p
            | ID: #{user._id} <br/>
            | Username: #{user.username} <br/>
            | Display Name: #{user.displayName} <br/>
            if user.emails
                | Email: #{user.emails[0].value} <br/>
        a.btn.btn-sm.btn-danger(href="/logout") Log out